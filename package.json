{
  "name": "type-script-node-express",
  "version": "1.0.0",
  "description": "TypeScript Node Express sample app",
  "main": "server.ts",
  "scripts": {
    "build": "rm -rf ./dist && node ./node_modules/typescript/bin/tsc",
    "dev": "node ./node_modules/nodemon/bin/nodemon --config ./nodemon.json",
    "start": "node ./node_modules/nodemon/bin/nodemon ./dist/server.js",
    "prod": "npm run build && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "node ./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TameshwarNirmalkar/TypeScriptNodeExpress.git"
  },
  "keywords": [
    "Express",
    "TypeScript",
    "ts",
    "ex",
    "expressjs",
    "overnight",
    "overnightjs",
    "node",
    "web",
    "webserver",
    "server"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TameshwarNirmalkar/TypeScriptNodeExpress/issues"
  },
  "homepage": "https://github.com/TameshwarNirmalkar/TypeScriptNodeExpress#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.0",
    "simple-color-print": "^1.0.6",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@overnightjs/core": "^1.5.0",
    "@overnightjs/logger": "^1.0.6",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/mongodb": "^3.1.28",
    "@types/mongoose": "^5.5.6"
  }
}
